{
    "name": "larabug/larabug",
    "description": "Laravel 6.x/8.x bug notifier",
    "keywords": [
        "laravel",
        "log",
        "error"
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "guzzlehttp/guzzle": "^6.0.2 || ^7.0",
        "illuminate/support": "^6.0 || ^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^4.0 || ^6.0",
        "mockery/mockery": "^1.3.3 || ^1.4.2"
    },
    "autoload": {
        "psr-4": {
            "LaraBug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraBug\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraBug\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Dennis Smink",
            "email": "dennissmink@gmail.com",
            "homepage": "http://www.dennissmink.nl",
            "role": "Owner"
        }
    ]
}
